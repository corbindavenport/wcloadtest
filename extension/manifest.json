{
  "manifest_version": 2,
  "name": "WC Power Test",
  "description": "Customized version of Google's Chromium power_LoadTest extension.",
  "homepage_url": "https://github.com/corbindavenport/wcloadtest",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/wcatter.png",
    "default_title": "Start Power Test"
  },
  "icons": {
    "48": "img/wcatter.png"
  },
  "permissions": [
    "webRequest",
    "http://*/",
    "https://*/",
    "tabs",
    "power"
  ],
  "background": {
    "persistent": true,
    "page": "bg.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "ct.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    "custom_event_listener.js"
  ]
}